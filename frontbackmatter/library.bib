file = {:Users/lento/Documents/Mendeley/Duraisamy, Iaccarino, Xiao/Duraisamy, Iaccarino, Xiao - 2018 - Turbulence Modeling in the Age of Data.pdf:pdf},
journal = {Annu. Rev. Fluid Mech.},
keywords = {data-driven,machine learning,modeling,statistical inference,turbulence modeling,uncertainty quantification},
pages = {357--377},
