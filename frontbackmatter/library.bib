file = {:Users/lento/Documents/Mendeley/Woodrum, Ewers, Telewski/Woodrum, Ewers, Telewski - 2003 - Hydraulic, biomechanical, and anatomical interactions of xylem from five species of Acer (Aceraceae).pdf:pdf},
isbn = {0002-9122},
