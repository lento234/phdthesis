@article{Richards1993,
abstract = {In computational models of wind engineering problems within the atmospheric surface layer the approach flow should normally be modelled as a homogeneous flow. Velocity and turbulence profiles associated with the k-ϵ turbulence model are proposed which produce homogeneous conditions. These equations are discussed in the light of full-scale measurements at Silsoe using sonic anemometers. It is suggested that the model constants k = 0.42, C$\mu$ = 0.013 and $\sigma$ϵ = 3.22 more closely match the data obtained. It is also shown that the cospectrum for the Reynolds stress exhibits a characteristics frequency no ≈ u∗/z which is consistent with the suggested profile equations.},
