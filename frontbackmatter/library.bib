file = {:Users/lento/Documents/Mendeley/MacMinn, Dufresne, Wettlaufer/MacMinn, Dufresne, Wettlaufer - 2015 - Fluid-driven deformation of a soft granular material.pdf:pdf},
issn = {21603308},
journal = {Phys. Rev. X},
keywords = {Fluid dynamics,Mechanics,Soft matter},
number = {1},
pages = {1--12},
title = {{Fluid-driven deformation of a soft granular material}},
volume = {5},
year = {2015}
}
