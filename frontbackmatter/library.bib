journal = {Philos. Trans. R. Soc. B Biol. Sci.},
number = {927},
pages = {593--610},
pmid = {1000105633},
title = {{The Interpretation of the Variations in Leaf Water Potential and Stomatal Conductance Found in Canopies in the Field}},
url = {http://rstb.royalsocietypublishing.org/cgi/doi/10.1098/rstb.1976.0035},
volume = {273},
