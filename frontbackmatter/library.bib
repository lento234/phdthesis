year = {2017}
}
@article{Honkanen2005,
abstract = {This study presents a simple image pre-processing scheme to extract$\backslash$nbackground information from double-frame particle image velocimetry$\backslash$n(PIV) images. Everything that stays stagnant in the image (e.g.,$\backslash$nimage background and light reflections from stationary objects) is$\backslash$nassumed to be a source of disturbance and is removed by subtracting$\backslash$nthe second frame of the image pair from the first frame. This yields$\backslash$na single frame difference image, which is transferred back to a background$\backslash$nextracted double-frame image. After the procedure the background$\backslash$nin the image does not correlate with itself anymore and therefore$\backslash$na bias error in PIV analysis towards zero displacement is avoided.$\backslash$nThe simulations show that the procedure conserves the profile of$\backslash$ntracer particle images when a displacement between the frames is$\backslash$nlarger than a particle image size. The performance of this procedure$\backslash$nis emphasized with various examples, and extensions of the procedure$\backslash$nare introduced. The extended procedure extracts background objects$\backslash$nthat move between the image frames, e.g. dispersed phase particles$\backslash$nin a two-phase flow or laser light sheet reflections from moving$\backslash$nobjects.},
author = {Honkanen, Markus and Nobach, Holger},
doi = {10.1007/s00348-004-0916-x},
file = {:Users/lento/Documents/Mendeley/Honkanen, Nobach/Honkanen, Nobach - 2005 - Background extraction from double-frame PIV images.pdf:pdf},
isbn = {0034800409},
