journal = {Asp. Appl. Biol.},
keywords = {airflow,canopy,canopy architecture,kEpsilon,orchard canopy},
mendeley-tags = {canopy,kEpsilon},
number = {1982},
pages = {465--472},
title = {{Modelling airflow through 3d canopy structure of orchards}},
