file = {:Users/lento/Documents/Mendeley/Seto, Guneralp, Hutyra/Seto, Guneralp, Hutyra - 2012 - Global forecasts of urban expansion to 2030 and direct impacts on biodiversity and carbon pools.pdf:pdf},
isbn = {0027-8424},
issn = {0027-8424},
journal = {Proc. Natl. Acad. Sci.},
number = {40},
pages = {16083--16088},
pmid = {22988086},
title = {{Global forecasts of urban expansion to 2030 and direct impacts on biodiversity and carbon pools}},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.1211658109},
