@article{Vrugt2001,
abstract = {Although solutions of multidimensional transient water flow can be$\backslash$nobtained by numerical modeling, their application may be limited$\backslash$nas root water uptake is generally considered to be one- or two-dimensional$\backslash$nonly. This is especially the case for trees. The first objective$\backslash$nof this paper is to test the suitability of a three-dimensional root$\backslash$nwater uptake model for the simultaneous simulation of transient soil$\backslash$nwater flow around an almond tree. The soil hydraulic and root water$\backslash$nuptake parameters were optimized by minimizing the residuals between$\backslash$nmeasured and simulated water content data. Water content was measured$\backslash$nin a three-dimensional grid around a sprinkler-irrigated almond tree$\backslash$nfor a 16 day period, following irrigation. A second objective was$\backslash$nto compare the performance and results of the three-dimensional flow$\backslash$nmodel with one- and two- dimensional root water uptake models. For$\backslash$nthis purpose, measured water contents were aggregated in the x and$\backslash$ny direction in the one-dimensional case and in the radial direction$\backslash$nfor the two-dimensional uptake model. For the estimation of root$\backslash$nwater uptake model parameters a genetic algorithm was used to estimate$\backslash$nthe approximate global minimum of the parameter space, whereas final$\backslash$nparameters were determined using the Simplex optimization algorithm.$\backslash$nWith the optimized root water uptake parameters, simulated and measured$\backslash$nwater contents during the 16-day period were in excellent agreement$\backslash$nfor all root water uptake models. Most significantly, the spatial$\backslash$nvariation in flux density below the rooting zone decreased when reducing$\backslash$nmultidimensional root water uptake to fewer dimensions, thereby justifying$\backslash$nthe proposed multidimensional approach.},
author = {Vrugt, J. A. and {Van Wijk}, M. T. and Hopmans, J. W. and {\v{S}}imunek, J.},
doi = {10.1029/2000WR000027},
file = {:Users/lento/Documents/Mendeley/Vrugt et al/Vrugt et al. - 2001 - One-, two-, and three-dimensional root water uptake functions for transient modeling.pdf:pdf},
isbn = {0043-1397},
issn = {00431397},
journal = {Water Resour. Res.},
keywords = {http://dx.doi.org/10.1029/2000WR000027, doi:10.102},
number = {10},
pages = {2457--2470},
pmid = {572106},
title = {{One-, two-, and three-dimensional root water uptake functions for transient modeling}},
volume = {37},
year = {2001}
}
@article{Dong2007,
abstract = {Porosity is the most important parameter that determines the efficiency of wind fences. The present study provided a deeper understanding of mean flow regime behind fences with different porosities at different wind velocities by means of a scaled wind tunnel simulation. Velocities were measured using particle image velocimetry and the mean velocity field was obtained and discussed. The mean velocity fields obtained at different wind velocities were similar. Analyzing the streamline patterns revealed an inherent link between fence porosity and mean airflow characteristics behind the fence. The optimal fence porosity is considered to be the critical porosity above or below which airflow characteristics differ strongly. According to the present study, the optimal porosity is found to be around 0.2 or 0.3, which corresponds to a critical porosity above which bleed flow dominates and below which reversed flow becomes significant. The parameters characterizing the reverse cell behind fences were well correlated with porosity. The velocity profiles revealed seven typical flow regions behind fences, characterized by different velocity gradients. The airflow becomes less complicated and the number of flow regions decreases as fence porosity increases. Some regions, especially the reverse cell and small vortex, disappear when the porosity exceeds a certain value. The flow regions gradually merge as the distance downwind increases, and eventually recover a single velocity profile due to downward transfer of momentum from overlying layers. The recovery distance decreases with increasing fence porosity. ?? 2007 Elsevier B.V. All rights reserved.},
author = {Dong, Zhibao and Luo, Wanyin and Qian, Guangqiang and Wang, Hongtao},
doi = {10.1016/j.agrformet.2007.05.009},
file = {:Users/lento/Documents/Mendeley/Dong et al/Dong et al. - 2007 - A wind tunnel simulation of the mean velocity fields behind upright porous fences.pdf:pdf},
issn = {01681923},
journal = {Agric. For. Meteorol.},
keywords = {Flow regions,Mean velocity regime,Optimal fence porosity,Streamline patterns,Upright porous fences},
number = {1-2},
pages = {82--93},
title = {{A wind tunnel simulation of the mean velocity fields behind upright porous fences}},
volume = {146},
year = {2007}
}
@article{Yousif2011,
abstract = {For accurate particle image velocimetry measurements in hemodynamics studies it is important to use a fluid with a refractive index (n) matching that of the vascular models (phantoms) and ideally a dynamic viscosity matching human blood. In this work, a blood-mimicking fluid (BMF) composed of water, glycerol, and sodium iodide was formulated for a range of refractive indices to match most common silicone elastomers (n=1.40-1.43) and with corresponding dynamic viscosity within the average cited range of healthy human blood (4.4±0.5 cP). Both refractive index and viscosity were attained at room temperature (22.2±0.2°C), which eliminates the need for a temperature-control system. An optimally matched BMF, suitable for use in a vascular phantom (n=1.4140±0.0008, Sylgard 184), was demonstrated with composition (by weight) of 47.38{\%} water, 36.94{\%} glycerol (44:56 glycerol-water ratio), and 15.68{\%} sodium iodide salt, resulting in a dynamic viscosity of 4.31±0.03 cP.},
