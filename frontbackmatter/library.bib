journal = {J. Agric. Eng. Res.},
month = {oct},
number = {2},
pages = {135--144},
title = {{Characterization and Modelling of the Air Fluxes induced by Natural Ventilation in a Greenhouse}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-0000321201{\&}partnerID=tZOtx3y1 http://linkinghub.elsevier.com/retrieve/pii/S0021863499904420},
volume = {74},
