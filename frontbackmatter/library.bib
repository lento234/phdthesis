isbn = {1537-5110},
issn = {15375110},
journal = {Biosyst. Eng.},
month = {mar},
number = {3},
pages = {301--312},
title = {{Optimisation of Greenhouse Insect Screening with Computational Fluid Dynamics}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S1537511005002655},
volume = {93},
year = {2006}
}
@article{Defraeye2014a,
abstract = {MAIN CONCLUSION: Quantitative neutron imaging is a promising technique to investigate leaf water flow and transpiration in real time and has perspectives towards studies of plant response to environmental conditions and plant water stress. The leaf hydraulic architecture is a key determinant of plant sap transport and plant-atmosphere exchange processes. Non-destructive imaging with neutrons shows large potential for unveiling the complex internal features of the venation network and the transport therein. However, it was only used for two-dimensional imaging without addressing flow dynamics and was still unsuccessful in accurate quantification of the amount of water. Quantitative neutron imaging was used to investigate, for the first time, the water distribution in veins and lamina, the three-dimensional venation architecture and sap flow dynamics in leaves. The latter was visualised using D2O as a contrast liquid. A high dynamic resolution was obtained by using cold neutrons and imaging relied on radiography (2D) as well as tomography (3D). The principle of the technique was shown for detached leaves, but can be applied to in vivo leaves as well. The venation network architecture and the water distribution in the veins and lamina unveiled clear differences between plant species. The leaf water content could be successfully quantified, though still included the contribution of the leaf dry matter. The flow measurements exposed the hierarchical structure of the water transport pathways, and an accurate quantification of the absolute amount of water uptake in the leaf was possible. Particular advantages of neutron imaging, as compared to X-ray imaging, were identified. Quantitative neutron imaging is a promising technique to investigate leaf water flow and transpiration in real time and has perspectives towards studies of plant response to environmental conditions and plant water stress.},
author = {Defraeye, Thijs and Derome, Dominique and Aregawi, Wondwosen and Cantr{\'{e}}, Dennis and Hartmann, Stefan and Lehmann, Eberhard and Carmeliet, Jan and Voisard, Fr{\'{e}}d{\'{e}}ric and Verboven, Pieter and Nicolai, Bart},
doi = {10.1007/s00425-014-2093-3},
file = {:Users/lento/Documents/Mendeley/Defraeye et al/Defraeye et al. - 2014 - Quantitative neutron imaging of water distribution, venation network and sap flow in leaves.pdf:pdf},
issn = {14322048},
journal = {Planta},
keywords = {Flow dynamics,Leaves,Neutron imaging,Non-destructive,Tomography,Venation},
number = {2},
pages = {423--436},
pmid = {24923675},
title = {{Quantitative neutron imaging of water distribution, venation network and sap flow in leaves}},
volume = {240},
year = {2014}
}
