pages = {84--93},
publisher = {Elsevier},
title = {{A semi-empirical model for the effect of trees on the urban wind environment}},
url = {http://dx.doi.org/10.1016/j.landurbplan.2017.09.029 https://linkinghub.elsevier.com/retrieve/pii/S0169204617302608},
