keywords = {Built environment,CFD validation,Pollutant dispersion,Turbulent Schmidt number,Vegetation},
pmid = {25463716},
title = {{Influence of avenue-trees on air quality at the urban neighborhood scale. Part I: Quality assurance studies and turbulent Schmidt number analysis for RANS CFD simulations}},
