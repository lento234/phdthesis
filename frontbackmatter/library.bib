isbn = {0894-8763},
issn = {0894-8763},
journal = {J. Appl. Meteorol.},
number = {10},
pages = {1392--1409},
title = {{Oblique, Stratified Winds about a Shelter Fence. Part II: Comparison of Measurements with Numerical Models}},
volume = {43},
year = {2004}
}
