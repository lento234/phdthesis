@article{Merlot2002,
abstract = {In response to drought, plants synthesise the hormone abscisic acid (ABA), which triggers closure of the stomatal pores. This process is vital for plants to conserve water by reducing transpirational water loss. Moreover, recent studies have demonstrated the advantages of the Arabidopsis stomatal guard cell for combining genetic, molecular and biophysical approaches to characterise ABA action. However, genetic dissection of stomatal regulation has been limited by the difficulty of identifying a reliable phenotype for mutant screening. Leaf temperature can be used as an indicator to detect mutants with altered stomatal control, since transpiration causes leaf cooling. In this study, we optimised experimental conditions under which individual Arabidopsis plants with altered stomatal responses to drought can be identified by infrared thermography. These conditions were then used to perform a pilot screen for mutants that displayed a reduced ability to close their stomata and hence appeared colder than the wild type. Some of the mutants recovered were deficient in ABA accumulation, and corresponded to alleles of the ABA biosynthesis loci ABA1, ABA2 and ABA3. Interestingly, two of these novel aba2 alleles were able to intragenically complement the aba2-1 mutation. The remaining mutants showed reduced ABA responsiveness in guard cells. In addition to the previously known abi1-1 mutation, we isolated mutations at two novel loci designated as OST1 (OPEN STOMATA 1) and OST2. Remarkably, ost1 and ost2 represent, to our knowledge, the first Arabidopsis mutations altering ABA responsiveness in stomata and not in seeds.},
author = {Merlot, Sylvain and Mustilli, Anna-Chiara and Genty, Bernard and North, Helen and Lefebvre, Val{\'{e}}rie and Sotta, Bruno and Vavasseur, Alain and Giraudat, J{\'{e}}r{\^{o}}me},
doi = {10.1046/j.1365-313X.2002.01322.x},
file = {:Users/lento/Documents/Mendeley/Merlot et al/Merlot et al. - 2002 - Use of infrared thermal imaging to isolate Arabidopsis mutants defective in stomatal regulation.pdf:pdf},
isbn = {1365-313X},
issn = {0960-7412},
journal = {Plant J.},
keywords = {Abscisic Acid,Abscisic Acid: metabolism,Alleles,Arabidopsis,Arabidopsis: cytology,Arabidopsis: genetics,Arabidopsis: physiology,Body Temperature,Desiccation,Genes,Infrared Rays,Mutation,Plant,Plant Leaves,Plant Leaves: cytology,Plant Leaves: genetics,Plant Leaves: physiology,Plant: genetics,Thermography,Water,Water: metabolism},
pages = {601--9},
pmid = {12047634},
title = {{Use of infrared thermal imaging to isolate Arabidopsis mutants defective in stomatal regulation.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/12047634},
volume = {30},
year = {2002}
}
@article{Medlyn2011,
abstract = {Models of vegetation function are widely used to predict the effects$\backslash$nof climate change on carbon, water and nutrient cycles of terrestrial$\backslash$necosystems, and their feedbacks to climate. Stomatal conductance,$\backslash$nthe process that governs plant water use and carbon uptake, is fundamental$\backslash$nto such models. In this paper, we reconcile two long-standing theories$\backslash$nof stomatal conductance. The empirical approach, which is most commonly$\backslash$nused in vegetation models, is phenomenological, based on experimental$\backslash$nobservations of stomatal behaviour in response to environmental conditions.$\backslash$nThe optimal approach is based on the theoretical argument that stomata$\backslash$nshould act to minimize the amount of water used per unit carbon gained.$\backslash$nWe reconcile these two approaches by showing that the theory of optimal$\backslash$nstomatal conductance can be used to derive a model of stomatal conductance$\backslash$nthat is closely analogous to the empirical models. Consequently,$\backslash$nwe obtain a unified stomatal model which has a similar form to existing$\backslash$nempirical models, but which now provides a theoretical interpretation$\backslash$nfor model parameter values. The key model parameter, g(1), is predicted$\backslash$nto increase with growth temperature and with the marginal water cost$\backslash$nof carbon gain. The new model is fitted to a range of datasets ranging$\backslash$nfrom tropical to boreal trees. The parameter g(1) is shown to vary$\backslash$nwith growth temperature, as predicted, and also with plant functional$\backslash$ntype. The model is shown to correctly capture responses of stomatal$\backslash$nconductance to changing atmospheric CO2, and thus can be used to$\backslash$ntest for stomatal acclimation to elevated CO2. The reconciliation$\backslash$nof the optimal and empirical approaches to modelling stomatal conductance$\backslash$nis important for global change biology because it provides a simple$\backslash$ntheoretical framework for analyzing, and simulating, the coupling$\backslash$nbetween carbon and water cycles under environmental change. },
author = {Medlyn, Belinda E. and Duursma, Remko A. and Eamus, Derek and Ellsworth, David S. and Prentice, I. Colin and Barton, Craig V.M. and Crous, Kristine Y. and {De Angelis}, Paolo and Freeman, Michael and Wingate, Lisa},
doi = {10.1111/j.1365-2486.2010.02375.x},
file = {:Users/lento/Documents/Mendeley/Medlyn et al/Medlyn et al. - 2011 - Reconciling the optimal and empirical approaches to modelling stomatal conductance.pdf:pdf},
isbn = {1354-1013},
issn = {13541013},
journal = {Glob. Chang. Biol.},
keywords = {Coupled conductance and photosynthesis models,Marginal water cost of carbon,Stomatal conductance,Stomatal optimization},
number = {6},
pages = {2134--2144},
pmid = {79955035036},
title = {{Reconciling the optimal and empirical approaches to modelling stomatal conductance}},
volume = {17},
year = {2011}
}
