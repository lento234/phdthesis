@article{Peramaki2005,
abstract = {A non-steady-state model of water tension propagation in tree stems was developed. The model is based on the cohesion theory and the assumption that fluctuating water tension driven by transpiration together with the elasticity of wood cause variations in the diameter of a tree stem. The change in xylem diameter can be linked to water tension in accordance with Hooke's law. The model was tested against field measurements of the diurnal change in xylem diameter at different heights in a 180-year-old Scots pine tree at Hyyti{\"{a}}l{\"{a}}, southern Finland. Model predictions agreed well with measurements. The effect of tree dimensions on pressure propagation was examined with the model. The model outcomes were also consistent with results of several field measurements presented in the literature.},
