@article{Boulard1999,
abstract = {Convective transfers determine, for a large part, the energy and mass balances which regulate the micro-climate inside a greenhouse. Airflows and temperature patterns induced by buoyancy forces through greenhouse roof openings were considered. Flow visualizations were performed on a half-scale test cell simulating the absorption of solar radiation at the floor surface of a single-span greenhouse. Steady regime temperatures and flow patterns were investigated for a single-sided roof vent and for two symmetrical roof vents. In all cases, it was observed that incoming air streams through the lower section of the openings swept over the greenhouse walls in a large convective loop before escaping through the upper sections of the openings. These natural convective patterns were numerically simulated by means of computational fluid dynamics. For both configurations, it was found that the shape of the convective loop and the mean components of the air velocity and temperature were generally in good agreement with the experimental results. A detailed comparison of the measured and computed scalar fields through an opening section could also be performed. Finally, it is suggested that these results could provide useful hints to improve the greenhouse and vent designs. {\textcopyright} 1999 Silsoe Research Institute.},
author = {Boulard, T. and Haxaire, R. and Lamrani, M.A. and Roy, J.C. and Jaffrin, A.},
doi = {10.1006/jaer.1999.0442},
