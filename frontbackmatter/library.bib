journal = {Urban Clim.},
keywords = {Computational fluid dynamics (CFD),Heat and moisture transport,Porous media,Radiation,Urban microclimate,Wind-driven rain},
month = {jun},
pages = {398--418},
publisher = {Elsevier Inc.},
title = {{Coupling of physical phenomena in urban microclimate: A model integrating air flow, wind-driven rain, radiation and transport in building materials}},
url = {http://dx.doi.org/10.1016/j.uclim.2017.04.012 https://linkinghub.elsevier.com/retrieve/pii/S2212095517300366},
volume = {24},
