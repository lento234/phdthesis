pages = {170--180},
publisher = {Elsevier Ltd},
title = {{Impact, runoff and drying of wind-driven rain on a window glass surface: Numerical modelling based on experimental validation}},
url = {http://dx.doi.org/10.1016/j.buildenv.2014.11.006},
volume = {84},
year = {2015}
}
@article{Baille1994,
abstract = {Canopy surface resistances (rs) for nine ornamental species (begonia, cyclamen, gardenia, gloxinia, hibiscus, impatiens, pelargonium, poinsettia and schefflera) were deduced from measurements of stand evapotranspiration (E) and used with the Penman-Monteith equation to predict hourly rates of E. A multiplicative relationship was used to express rs as a function of solar irradiance (G) and vapour pressure deficit (D). For most of the species, rs showed a high correlation with solar irradiance, except for begonia which had a significant correlation between rs and D, and poinsettia which had a low correlation of rs with both G and D. Minimum leaf stomatal resistance (rl,min) and dark resistance, rl,d were deduced from rs formulae. Significant differences were found between species: rl,min ranged from 150 s m-1 (impatiens) to 400 s m-1 (gloxinia), whereas rl,d varied from 1500 s m-1 (cyclamen) to 4500 s m-1 (schefflera). Calculated hourly E rates, gave good agreement with the experimental data. These results show that such a method based on the Penman-Monteith equation and the estimation of rs can be used to predict the short-term evapotranspiration rate of ornamental crops. ?? 1994.},
