journal = {Plant, Cell Environ.},
keywords = {3D architecture,Ecophysiology,Intracanopy scale,Juglans regia L.,Model description,Walnut tree},
number = {4},
pages = {395--406},
pmid = {4681},
title = {{RATP: A model for simulating the spatial distribution of radiation absorption, transpiration and photosynthesis within canopies: Application to an isolated tree crown}},
