journal = {Geotech. Test. J.},
keywords = {Aquabeads,Digital image processing,Educational,Geoenvironmental,Glass beads,Groundwater,Model,Multiphase flow,Napl,Silica gel,Silica powder,Surfactant flushing,Tank test,Transparent soil},
number = {1},
title = {{A transparent water-based polymer for simulating multiphase flow}},
volume = {33},
year = {2010}
