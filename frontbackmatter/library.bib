journal = {J. Wind Eng. Ind. Aerodyn.},
keywords = {Building energy simulation,Computational fluid dynamics,Convective heat transfer coefficient,Space cooling,Street canyon,Urban heat island},
pages = {464--473},
publisher = {Elsevier},
title = {{Analysis of convective heat transfer at building fa??ades in street canyons and its influence on the predictions of space cooling demand in buildings}},
url = {http://dx.doi.org/10.1016/j.jweia.2012.02.003},
